
<%flash.each do |flash_type ,message |%>
	<%if flash_type == "alert"%>
		<div class="flash_message alert alert-danger" role = "alert">
			<h6><%= message %></h6>
		</div>
	<%else%>
		<div class="flash_message alert alert-success" role = "alert">
			<h6><%= message %></h6>
		</div>
	<%end%> 
<%end%> 
